{
  "name": "knex-mysql-middleware",
  "version": "0.3.0",
  "description": "Set a MySQL flavored Knex connection that can be accessed via Koa or Express middleware.",
  "main": "koa.js",
  "scripts": {
    "test": "NODE_ENV=test mocha --harmony --reporter spec",
    "develop": "rerun-script"
  },
  "watches": [
    {
      "script": "test",
      "patterns": [
        "*.js",
        "lib/**/*.js",
        "test/**/*.js"
      ]
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/wilmoore/knex-mysql-middleware"
  },
  "files": [
    "koa.js",
    "express.js",
    "license",
    "package.json"
  ],
  "keywords": [
    "ecosystem:koa",
    "ecosystem:express",
    "ecosystem:knex",
    "dotenv",
    "envc",
    "knex",
    "mysql",
    "database",
    "middleware",
    "koa",
    "express"
  ],
  "author": "Wil Moore III <wil.moore@wilmoore.com>",
  "license": "MIT",
  "devDependencies": {
    "chai": "^1.10.0",
    "eslint": "^0.10.1",
    "express": "^4.10.4",
    "koa": "^0.12.2",
    "mocha": "^2.0.1",
    "rerun-script": "^0.4.1"
  },
  "dependencies": {
    "debug": "^2.1.0",
    "knex": "^0.7.3",
    "mysql": "^2.5.3"
  }
}
